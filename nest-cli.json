      "entryFile": "index",
      "sourceRoot": "libs/services/src",
      "compilerOptions": {
        "tsConfigPath": "libs/services/tsconfig.lib.json"
      }
    },
    "sync-worker": {
      "type": "application",
      "root": "apps/sync-worker",
      "entryFile": "main",
      "sourceRoot": "apps/sync-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sync-worker/tsconfig.app.json"
      }
    }
  }
}