{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/inventory/src",
  "compilerOptions": {
    "webpack": true,
    "deleteOutDir": true,
    "sourceMap": false,
    "minify": false,
    "tsConfigPath": "apps/inventory/tsconfig.app.json"
  },
  "builder": {
    "type": "swc",
    "options": {
      "swcrcPath": ".swcrc"
    }
  },
  "monorepo": true,
  "root": "apps/inventory",
  "projects": {
    "cache": {
      "type": "library",
      "root": "libs/cache",
      "entryFile": "index",
      "sourceRoot": "libs/cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache/tsconfig.lib.json"
      }
    },
    "cli": {
      "type": "application",
      "root": "apps/cli",
      "entryFile": "main",
      "sourceRoot": "apps/cli/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cli/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "control-unit-lock": {
      "type": "library",
      "root": "libs/control-unit-lock",
      "entryFile": "index",
      "sourceRoot": "libs/control-unit-lock/src",
      "compilerOptions": {
        "tsConfigPath": "libs/control-unit-lock/tsconfig.lib.json"
      }
    },
    "dals": {
      "type": "library",
      "root": "libs/dals",
      "entryFile": "index",
